<h1>Galpão <%= @warehouse.code %></h1>
<h2><%= @warehouse.description %></h2>

<div>
  <strong>Nome: </strong><%= @warehouse.name %>
  <strong>Cidade: </strong><%= @warehouse.city %>
  <strong>Área: </strong><%= number_with_delimiter(@warehouse.area, delimiter: '.') %> m²
  <strong>Endereço: </strong><%= @warehouse.address %> <strong>CEP: </strong><%= @warehouse.postal_code %>
</div>

<%= link_to('Voltar', root_path) %>