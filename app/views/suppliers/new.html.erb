<h1 class="my-3">Novo Fornecedor</h1>


<% if(@supplier.errors.any?) %>
  <div class="alert alert-danger" role="alert">
    <p>Verifique os erros abaixo:</p>
    <ul>
      <% @supplier.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
    </ul>
  </div>
<% end %>


